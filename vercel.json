{
  "version": 2,
  "builds": [
    {
      "src": "web/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "web/main.py" },
    { "source": "/(.*)", "destination": "web/main.py" }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/web",
      "permanent": true
    }
  ]
}