{
  "version": 2,
  "builds": [
    {
      "src": "web/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web/index.html"
    },
    {
      "src": "/web",
      "dest": "/web/index.html"
    },
    {
      "src": "/web/(.*)",
      "dest": "/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/main.py"
    }
  ]
}